{
  "name": "sales-tracker-server",
  "version": "1.0.0",
  "description": "Backend server for Sales Tracker Application",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:create": "psql -U ${PG_USER:-postgres} -c 'CREATE DATABASE ${PG_DATABASE:-sales_tracker};' || echo 'Database may already exist'",
    "db:setup": "psql -U ${PG_USER:-postgres} -d ${PG_DATABASE:-sales_tracker} -f db/schema.sql",
    "db:seed": "psql -U ${PG_USER:-postgres} -d ${PG_DATABASE:-sales_tracker} -f db/seed.sql",
    "db:reset": "npm run db:setup && npm run db:seed",
    "db:init": "npm run db:create && npm run db:reset"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "puppeteer": "^24.25.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
